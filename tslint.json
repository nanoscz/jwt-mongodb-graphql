{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "no-console": [false],
    "import-name": [true, { "database": "Database", "jwt": "JWT" }]
  }
}
